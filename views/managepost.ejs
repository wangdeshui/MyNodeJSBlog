<table class="table table-bordered">
    <thead>
    <tr>
        <th>Title</th>
        <th></th>
    </tr>

    </thead>
    <tbody>
    <% for(post in posts){ %>
    <tr>
        <td><a href='/post/<%= posts[post].id%>'><strong> <%= posts[post].title %></strong></a></td>
